    <tags>accord.net framework extensions math statistics</tags>

    <dependencies>
      <dependency id="Accord.Statistics"         version="2.14" />

      <dependency id="DotImaging.Primitives2D"   version="2.5.3" />
      
      <dependency id="Accord.Extensions.Math"    version="$version$" />
    </dependencies>

    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Numerics"  />
    </frameworkAssemblies>
  </metadata>
  
  <files>
    <file src="..\..\..\bin\Release\Accord.Extensions.Statistics.dll" target="lib\net45\Accord.Extensions.Statistics.dll" />
    <file src="..\..\..\bin\Release\Accord.Extensions.Statistics.xml" target="lib\net45\Accord.Extensions.Statistics.xml" />
  </files>
</package>